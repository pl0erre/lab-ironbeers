<div class="beer-container-random">
  <img src={{this.image_url}}>
  <div class="about-container">
    <h3>{{this.name}}</h3>
    <h4>{{this.tagline}}</h4>
    <p>{{this.description}}</p>
    <h5>Food Pairing</h5>
    <ul>{{#each this.food_pairing}}
          <li>{{this}}</li>
        {{/each}}
    </ul>
    <h5>Brewers Tips</h5>
    <p>{{this.brewers_tips}}</p>
  </div>
  
</div>